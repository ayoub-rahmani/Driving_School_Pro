<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.shape.SVGPath?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>

<BorderPane prefHeight="900.0" prefWidth="1600.0"
            styleClass="examen-container"
            stylesheets="@../Styles/Common.css,@../Styles/Examen.css"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.Controllers.ExamenController">

    <top>
        <!-- Header Section -->
        <HBox alignment="CENTER" spacing="20.0" styleClass="header" BorderPane.alignment="CENTER">
            <children>
                <!-- Back Button -->
                <Button fx:id="BackBtn" mnemonicParsing="false" onAction="#handleBack"
                        styleClass="icon-button">
                    <tooltip>
                        <Tooltip text="Retour"/>
                    </tooltip>
                    <graphic>
                        <SVGPath content="M20,11V13H8L13.5,18.5L12.08,19.92L4.16,12L12.08,4.08L13.5,5.5L8,11H20Z"
                                 fill="white" scaleX="1.2" scaleY="1.2"/>
                    </graphic>
                </Button>

                <Region HBox.hgrow="ALWAYS"/>

                <!-- Exam Icon and Title -->
                <SVGPath
                        content="M16,9H21.5L16,3.5V9M7,2H17L23,8V18A2,2 0 0,1 21,20H7C5.89,20 5,19.1 5,18V4A2,2 0 0,1 7,2M3,6V22H21V24H3A2,2 0 0,1 1,22V6H3Z"
                        fill="brown" scaleX="1.5" scaleY="1.5"/>
                <Label styleClass="header-label" text="GESTION DES EXAMENS"/>

                <Region HBox.hgrow="ALWAYS"/>

                <!-- Dashboard Button -->
                <Button fx:id="dashboardBtn" mnemonicParsing="false" onAction="#toggleDashboard"
                        styleClass="icon-button">
                    <tooltip>
                        <Tooltip text="Tableau de bord"/>
                    </tooltip>
                    <graphic>
                        <SVGPath content="M3,11H11V3H3M3,21H11V13H3M13,21H21V13H13M13,3V11H21V3"
                                 fill="white" scaleX="1.2" scaleY="1.2"/>
                    </graphic>
                </Button>

                <!-- Settings Button (NEW) -->
                <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#handleSettings"
                        styleClass="icon-button">
                    <tooltip>
                        <Tooltip text="Paramètres"/>
                    </tooltip>
                    <graphic>
                        <SVGPath content="M12,15.5A3.5,3.5 0 0,1 8.5,12A3.5,3.5 0 0,1 12,8.5A3.5,3.5 0 0,1 15.5,12A3.5,3.5 0 0,1 12,15.5M19.43,12.97C19.47,12.65 19.5,12.33 19.5,12C19.5,11.67 19.47,11.34 19.43,11L21.54,9.37C21.73,9.22 21.78,8.95 21.66,8.73L19.66,5.27C19.54,5.05 19.27,4.96 19.05,5.05L16.56,6.05C16.04,5.66 15.5,5.32 14.87,5.07L14.5,2.42C14.46,2.18 14.25,2 14,2H10C9.75,2 9.54,2.18 9.5,2.42L9.13,5.07C8.5,5.32 7.96,5.66 7.44,6.05L4.95,5.05C4.73,4.96 4.46,5.05 4.34,5.27L2.34,8.73C2.21,8.95 2.27,9.22 2.46,9.37L4.57,11C4.53,11.34 4.5,11.67 4.5,12C4.5,12.33 4.53,12.65 4.57,12.97L2.46,14.63C2.27,14.78 2.21,15.05 2.34,15.27L4.34,18.73C4.46,18.95 4.73,19.03 4.95,18.95L7.44,17.94C7.96,18.34 8.5,18.68 9.13,18.93L9.5,21.58C9.54,21.82 9.75,22 10,22H14C14.25,22 14.46,21.82 14.5,21.58L14.87,18.93C15.5,18.67 16.04,18.34 16.56,17.94L19.05,18.95C19.27,19.03 19.54,18.95 19.66,18.73L21.66,15.27C21.78,15.05 21.73,14.78 21.54,14.63L19.43,12.97Z"
                                 fill="white" scaleX="1.2" scaleY="1.2"/>
                    </graphic>
                </Button>

                <!-- Help Button (NEW) -->
                <Button fx:id="helpBtn" mnemonicParsing="false" onAction="#handleHelp"
                        styleClass="icon-button">
                    <tooltip>
                        <Tooltip text="Aide"/>
                    </tooltip>
                    <graphic>
                        <SVGPath content="M11,18H13V16H11V18M12,2A10,10 0 0,0 2,12A10,10 0 0,0 12,22A10,10 0 0,0 22,12A10,10 0 0,0 12,2M12,20C7.59,20 4,16.41 4,12C4,7.59 7.59,4 12,4C16.41,4 20,7.59 20,12C20,16.41 16.41,20 12,20M12,6A4,4 0 0,0 8,10H10A2,2 0 0,1 12,8A2,2 0 0,1 14,10C14,12 11,11.75 11,15H13C13,12.75 16,12.5 16,10A4,4 0 0,0 12,6Z"
                                 fill="white" scaleX="1.2" scaleY="1.2"/>
                    </graphic>
                </Button>
            </children>
            <padding>
                <Insets bottom="15.0" left="20.0" right="20.0" top="15.0"/>
            </padding>
        </HBox>
    </top>

    <left>
        <!-- Left Panel with Search, Table, Filters, and Export -->
        <ScrollPane styleClass="left-panel-scroll" fitToWidth="true">
            <VBox prefWidth="400.0" spacing="20.0" styleClass="examen-table-container" BorderPane.alignment="CENTER">
                <children>
                    <!-- Search Bar -->
                    <HBox alignment="CENTER_LEFT" spacing="10.0">
                        <children>
                            <TextField fx:id="rechercheField" promptText="Rechercher par CIN ou nom..."
                                       HBox.hgrow="ALWAYS" styleClass="search-box" onKeyReleased="#handleRecherche"/>
                            <Button fx:id="rechercheBtn" mnemonicParsing="false" onAction="#handleRecherche"
                                    styleClass="icon-button">
                                <tooltip>
                                    <Tooltip text="Rechercher"/>
                                </tooltip>
                                <graphic>
                                    <SVGPath
                                            content="M9.5,3A6.5,6.5 0 0,1 16,9.5C16,11.11 15.41,12.59 14.44,13.73L14.71,14H15.5L20.5,19L19,20.5L14,15.5V14.71L13.73,14.44C12.59,15.41 11.11,16 9.5,16A6.5,6.5 0 0,1 3,9.5A6.5,6.5 0 0,1 9.5,3M9.5,5C7,5 5,7 5,9.5C5,12 7,14 9.5,14C12,14 14,12 14,9.5C14,7 12,5 9.5,5Z"
                                            fill="#2563eb"/>
                                </graphic>
                            </Button>
                        </children>
                    </HBox>

                    <!-- Examen Table View -->
                    <TableView fx:id="examenTable" VBox.vgrow="ALWAYS">
                        <columns>
                            <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID"/>
                            <TableColumn fx:id="typeExamenColumn" prefWidth="120.0" text="Type"/>
                            <TableColumn fx:id="candidatNameColumn" prefWidth="140.0" text="Candidat"/>
                            <TableColumn fx:id="candidatCINColumn" prefWidth="120.0" text="CIN"/>
                            <TableColumn fx:id="dateExamenColumn" prefWidth="120.0" text="Date"/>
                        </columns>
                        <placeholder>
                            <Label text="Aucun examen trouvé"/>
                        </placeholder>
                    </TableView>

                    <!-- Action Buttons -->
                    <HBox alignment="CENTER" spacing="15.0">
                        <children>
                            <Button fx:id="ajouterBtn" mnemonicParsing="false" onAction="#handleAjouter"
                                    styleClass="button, button-success" text="Ajouter">
                                <tooltip>
                                    <Tooltip text="Ajouter un nouvel examen"/>
                                </tooltip>
                                <graphic>
                                    <SVGPath content="M19,13H13V19H11V13H5V11H11V5H13V11H19V13Z"
                                             fill="white"/>
                                </graphic>
                            </Button>
                            <Button fx:id="modifierBtn" mnemonicParsing="false" onAction="#handleModifier"
                                    styleClass="button, button-warning" text="Modifier">
                                <tooltip>
                                    <Tooltip text="Modifier l'examen sélectionné"/>
                                </tooltip>
                                <graphic>
                                    <SVGPath
                                            content="M20.71,7.04C21.1,6.65 21.1,6 20.71,5.63L18.37,3.29C18,2.9 17.35,2.9 16.96,3.29L15.12,5.12L18.87,8.87M3,17.25V21H6.75L17.81,9.93L14.06,6.18L3,17.25Z"
                                            fill="white"/>
                                </graphic>
                            </Button>
                            <Button fx:id="supprimerBtn" mnemonicParsing="false" onAction="#handleSupprimer"
                                    styleClass="button, button-danger" text="Supprimer">
                                <tooltip>
                                    <Tooltip text="Supprimer l'examen sélectionné"/>
                                </tooltip>
                                <graphic>
                                    <SVGPath
                                            content="M19,4H15.5L14.5,3H9.5L8.5,4H5V6H19M6,19A2,2 0 0,0 8,21H16A2,2 0 0,0 18,19V7H6V19Z"
                                            fill="white"/>
                                </graphic>
                            </Button>
                        </children>
                    </HBox>

                    <!-- Filters TitledPane -->
                    <TitledPane animated="true" text="Filtres" expanded="false">
                        <content>
                            <VBox spacing="10" style="-fx-padding: 5;">
                                <!-- Section Title -->
                                <Label text="Critères de recherche" styleClass="section-title"
                                       style="-fx-font-size: 14px; -fx-font-weight: bold;"/>

                                <!-- Type Filter -->
                                <VBox spacing="5">
                                    <Label text="Type d'examen" styleClass="filter-label"
                                           style="-fx-font-weight: bold;"/>
                                    <ComboBox fx:id="filterTypeComboBox" maxWidth="Infinity" promptText="Sélectionnez un type d'examen"/>
                                </VBox>

                                <!-- Date Range Filter -->
                                <VBox spacing="5">
                                    <Label text="Période" styleClass="filter-label"
                                           style="-fx-font-weight: bold;"/>
                                    <HBox spacing="10">
                                        <VBox spacing="3" HBox.hgrow="ALWAYS" maxWidth="Infinity">
                                            <Label text="Du:" style="-fx-font-size: 12px;"/>
                                            <DatePicker fx:id="startDatePicker" maxWidth="Infinity" promptText="Début"/>
                                        </VBox>
                                        <VBox spacing="3" HBox.hgrow="ALWAYS" maxWidth="Infinity">
                                            <Label text="Au:" style="-fx-font-size: 12px;"/>
                                            <DatePicker fx:id="endDatePicker" maxWidth="Infinity" promptText="Fin"/>
                                        </VBox>
                                    </HBox>
                                </VBox>

                                <!-- Status Filter -->
                                <VBox spacing="5">
                                    <Label text="Statut" styleClass="filter-label"
                                           style="-fx-font-weight: bold;"/>
                                    <ComboBox fx:id="filterStatusComboBox" maxWidth="Infinity" promptText="Sélectionnez un statut"/>
                                </VBox>

                                <!-- Divider -->
                                <Separator/>

                                <!-- Apply and Reset Buttons -->
                                <HBox alignment="CENTER" spacing="15">
                                    <Button fx:id="resetFilterBtn" minWidth="120" mnemonicParsing="false" text="Réinitialiser"
                                            styleClass="button-secondary" onAction="#handleResetFilter">
                                        <tooltip>
                                            <Tooltip text="Réinitialiser tous les filtres"/>
                                        </tooltip>
                                    </Button>
                                    <Button fx:id="applyFilterBtn" minWidth="120" mnemonicParsing="false" text="Appliquer"
                                            styleClass="button-primary" onAction="#handleApplyFilter">
                                        <tooltip>
                                            <Tooltip text="Appliquer les filtres sélectionnés"/>
                                        </tooltip>
                                    </Button>
                                </HBox>
                            </VBox>
                        </content>
                    </TitledPane>

                    <!-- Export Button -->
                    <Button fx:id="exportBtn" mnemonicParsing="false" onAction="#handleExport"
                            styleClass="button" text="Exporter les données" maxWidth="Infinity">
                        <tooltip>
                            <Tooltip text="Exporter les données de l'examen"/>
                        </tooltip>
                        <graphic>
                            <SVGPath
                                    content="M23,12L19,8V11H10V13H19V16M1,18V6C1,4.89 1.9,4 3,4H15A2,2 0 0,1 17,6V9H15V6H3V18H15V15H17V18A2,2 0 0,1 15,20H3A2,2 0 0,1 1,18Z"
                                    fill="white"/>
                        </graphic>
                    </Button>
                </children>
            </VBox>
        </ScrollPane>
    </left>

    <center>
        <!-- Central Content Area -->
        <StackPane>
            <children>
                <!-- Form ScrollPane -->
                <ScrollPane fx:id="formScrollPane" fitToHeight="true" fitToWidth="true"
                            styleClass="content-background">
                    <content>
                        <VBox spacing="20.0" styleClass="examen-form">
                            <children>
                                <!-- Examen Header -->
                                <HBox alignment="CENTER_LEFT" spacing="20" styleClass="examen-header">
                                    <!-- Exam Icon -->
                                    <ImageView fx:id="ExamenIcon" fitHeight="120.0" fitWidth="120.0"
                                               pickOnBounds="true" preserveRatio="true" styleClass="photo-placeholder">
                                        <image>
                                            <Image url="@../images/Examen.jpg"/>
                                        </image>
                                    </ImageView>

                                    <!-- Exam Information -->
                                    <VBox spacing="5">
                                        <Label styleClass="section-label" text="Informations d'Examen"/>
                                        <StackPane fx:id="examTypeBadgeContainer" alignment="CENTER_LEFT">
                                            <Label fx:id="examTypeBadge" styleClass="exam-type-badge" text="Type d'examen"/>
                                        </StackPane>
                                    </VBox>
                                </HBox>

                                <!-- Détails de l'Examen TitledPane -->
                                <TitledPane animated="true" text="Détails de l'Examen" expanded="true">
                                    <content>
                                        <GridPane hgap="20.0" vgap="20.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="150.0"
                                                                   prefWidth="150.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="150.0"
                                                                   prefWidth="150.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <!-- First row -->
                                                <Label text="Sélectionner Candidat:" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                                                <ComboBox fx:id="candidatComboBox" GridPane.columnIndex="1" GridPane.rowIndex="0"
                                                          prefWidth="300.0" promptText="Sélectionner un candidat"/>

                                                <Label text="Type d'examen:" GridPane.columnIndex="2" GridPane.rowIndex="0"/>
                                                <ComboBox fx:id="typeExamenComboBox" GridPane.columnIndex="3" GridPane.rowIndex="0"
                                                          prefWidth="150.0"/>

                                                <!-- Second row -->
                                                <Label text="Date d'examen:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                                                <DatePicker fx:id="dateExamenPicker" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

                                                <Label text="Lieu d'examen:" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                                                <TextField fx:id="lieuExamenField" GridPane.columnIndex="3" GridPane.rowIndex="1"/>


                                            </children>
                                            <padding>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </padding>
                                        </GridPane>
                                    </content>
                                </TitledPane>

                                <!-- Informations du Candidat TitledPane -->
                                <TitledPane animated="true" text="Informations du Candidat" expanded="false">
                                    <content>
                                        <GridPane hgap="20.0" vgap="20.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="150.0"
                                                                   prefWidth="150.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="150.0"
                                                                   prefWidth="150.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <Label text="Nom:"/>
                                                <TextField fx:id="nomCandidatField" GridPane.columnIndex="1"
                                                           editable="false"/>

                                                <Label text="Prénom:" GridPane.columnIndex="2"/>
                                                <TextField fx:id="prenomCandidatField" GridPane.columnIndex="3"
                                                           editable="false"/>

                                                <Label text="CIN:" GridPane.rowIndex="1"/>
                                                <TextField fx:id="cinCandidatField" GridPane.columnIndex="1"
                                                           GridPane.rowIndex="1" editable="false"/>

                                                <Label text="Téléphone:" GridPane.columnIndex="2"
                                                       GridPane.rowIndex="1"/>
                                                <TextField fx:id="telephoneCandidatField"
                                                           GridPane.columnIndex="3" GridPane.rowIndex="1"
                                                           editable="false"/>

                                                <Label text="Catégorie permis:" GridPane.rowIndex="2"/>
                                                <TextField fx:id="categorieCandidatField" GridPane.columnIndex="1"
                                                           GridPane.rowIndex="2" editable="false"/>

                                                <Label text="Date d'inscription:" GridPane.columnIndex="2"
                                                       GridPane.rowIndex="2"/>
                                                <TextField fx:id="dateInscriptionCandidatField"
                                                           GridPane.columnIndex="3" GridPane.rowIndex="2"
                                                           editable="false"/>

                                                <Label text="Statut:" GridPane.rowIndex="3"/>
                                                <TextField fx:id="statutCandidatField" GridPane.columnIndex="1"
                                                           GridPane.rowIndex="3" editable="false"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                                            </padding>
                                        </GridPane>
                                    </content>
                                </TitledPane>

                                <!-- Action Buttons -->
                                <HBox alignment="CENTER_RIGHT" spacing="15.0">
                                    <children>
                                        <Button fx:id="annulerBtn" mnemonicParsing="false" onAction="#handleAnnuler"
                                                styleClass="button, button-secondary" text="Annuler">
                                            <tooltip>
                                                <Tooltip text="Annuler les modifications"/>
                                            </tooltip>
                                            <graphic>
                                                <SVGPath
                                                        content="M19,6.41L17.59,5L12,10.59L6.41,5L5,6.41L10.59,12L5,17.59L6.41,19L12,13.41L17.59,19L19,17.59L13.41,12L19,6.41Z"
                                                        fill="white"/>
                                            </graphic>
                                        </Button>
                                        <Button fx:id="enregistrerBtn" mnemonicParsing="false"
                                                onAction="#handleEnregistrer"
                                                styleClass="button" text="Enregistrer">
                                            <tooltip>
                                                <Tooltip text="Enregistrer les informations de l'examen"/>
                                            </tooltip>
                                            <graphic>
                                                <SVGPath
                                                        content="M15,9H5V5H15M12,19A3,3 0 0,1 9,16A3,3 0 0,1 12,13A3,3 0 0,1 15,16A3,3 0 0,1 12,19M17,3H5C3.89,3 3,3.9 3,5V19A2,2 0 0,0 5,21H19A2,2 0 0,0 21,19V7L17,3Z"
                                                        fill="white"/>
                                            </graphic>
                                        </Button>
                                    </children>
                                    <padding>
                                        <Insets top="15.0"/>
                                    </padding>
                                </HBox>
                            </children>
                            <padding>
                                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                            </padding>
                        </VBox>
                    </content>
                </ScrollPane>

                <!-- Dashboard Panel -->
                <VBox fx:id="dashboardPane" spacing="20.0" styleClass="dashboard-container" visible="false">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>

                    <!-- Dashboard Header -->
                    <HBox alignment="CENTER_LEFT" spacing="20" styleClass="examen-header">
                        <StackPane styleClass="dashboard-icon">
                            <SVGPath content="M3,11H11V3H3M3,21H11V13H3M13,21H21V13H13M13,3V11H21V3"
                                     fill="white" scaleX="1.5" scaleY="1.5"/>
                        </StackPane>
                        <Label styleClass="section-label" text="Tableau de Bord"/>
                    </HBox>

                    <!-- Stats Cards -->
                    <HBox spacing="20" alignment="CENTER">
                        <VBox styleClass="stat-card" HBox.hgrow="ALWAYS">
                            <Label styleClass="stat-title" text="Total des examens"/>
                            <Label fx:id="totalExamensDashboardLabel" styleClass="stat-value" text="0"/>
                        </VBox>

                        <VBox styleClass="stat-card" HBox.hgrow="ALWAYS">
                            <Label styleClass="stat-title" text="Examens validés"/>
                            <Label fx:id="examensValidesDashboardLabel" styleClass="stat-value" text="0"/>
                        </VBox>

                        <VBox styleClass="stat-card" HBox.hgrow="ALWAYS">
                            <Label styleClass="stat-title" text="Examens en attente"/>
                            <Label fx:id="examensEnAttenteDashboardLabel" styleClass="stat-value" text="0"/>
                        </VBox>
                    </HBox>

                    <!-- Charts -->
                    <HBox spacing="20" VBox.vgrow="ALWAYS">
                        <VBox spacing="10" HBox.hgrow="ALWAYS" styleClass="chart-container">
                            <Label styleClass="chart-title" text="Répartition par type d'examen"/>
                            <PieChart fx:id="typeExamenChart" legendVisible="true" VBox.vgrow="ALWAYS"/>
                        </VBox>

                        <VBox spacing="10" HBox.hgrow="ALWAYS" styleClass="chart-container">
                            <Label styleClass="chart-title" text="Examens par mois"/>
                            <BarChart fx:id="examensParMoisChart" legendVisible="false" VBox.vgrow="ALWAYS">
                                <xAxis>
                                    <CategoryAxis label="Mois"/>
                                </xAxis>
                                <yAxis>
                                    <NumberAxis label="Nombre d'examens"/>
                                </yAxis>
                            </BarChart>
                        </VBox>
                    </HBox>

                    <!-- Refresh Button -->
                    <HBox alignment="CENTER_RIGHT" spacing="15.0">
                        <Button fx:id="refreshDashboardBtn" mnemonicParsing="false" onAction="#refreshDashboard"
                                styleClass="button" text="Actualiser">
                            <graphic>
                                <SVGPath
                                        content="M17.65,6.35C16.2,4.9 14.21,4 12,4A8,8 0 0,0 4,12A8,8 0 0,0 12,20C15.73,20 18.84,17.45 19.73,14H17.65C16.83,16.33 14.61,18 12,18A6,6 0 0,1 6,12A6,6 0 0,1 12,6C13.66,6 15.14,6.69 16.22,7.78L13,11H20V4L17.65,6.35Z"
                                        fill="white"/>
                            </graphic>
                        </Button>
                    </HBox>
                </VBox>
            </children>
        </StackPane>
    </center>
</BorderPane>

